list=[]
n=int(input("Enter the number of tuples: "))
for i in range (n):
    a=int(input("Enter the first value: "))
    b=int(input("Enter the second value: "))
    list.append((a,b))
for i in range(n):
    for j in range(n-1):
        if(list[j][1]>list[j-1][1]):
            list[j],list[j+1]=list[j+1],list[j]
print(list)
